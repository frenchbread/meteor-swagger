  "apis": [
    {
      "operations": [
        {
          "nickname": "getRelatedWords",
          "httpMethod": "GET",
          "errorResponses": [
            {
              "reason": "Invalid word supplied.",
              "code": 400
            }
          ],
          "responseClass": "List[related]",
          "parameters": [
            {
              "required": true,
              "dataType": "string",
              "name": "word",
              "paramType": "path",
              "allowMultiple": false,
              "description": "Word to fetch relationships for"
            },
            {
              "required": false,
              "dataType": "string",
              "name": "useCanonical",
              "defaultValue": "false",
              "allowableValues": {
                "values": [
                  "false",
                  "true"
                ],
                "valueType": "LIST"
              },
              "paramType": "query",
              "allowMultiple": false,
              "description": "If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested."
            },
            {
              "required": false,
              "dataType": "string",
              "name": "relationshipTypes",
              "allowableValues": {
                "values": [
                  "synonym",
                  "antonym",
                  "variant",
                  "equivalent",
                  "cross-reference",
                  "related-word",
                  "rhyme",
                  "form",
                  "etymologically-related-term",
                  "hypernym",
                  "hyponym",
                  "inflected-form",
                  "primary",
                  "same-context",
                  "verb-form",
                  "verb-stem",
                  "has_topic"
                ],
                "valueType": "LIST"
              },
              "paramType": "query",
              "allowMultiple": false,
              "description": "Limits the total results per type of relationship type"
            },
            {
              "required": false,
              "dataType": "int",
              "name": "limitPerRelationshipType",
              "defaultValue": "10",
              "allowableValues": {
                "min": 1.0,
                "max": "Infinity",
                "valueType": "RANGE"
              },
              "paramType": "query",
              "allowMultiple": false,
              "description": "Restrict to the supplied relatinship types"
            }
          ],
          "summary": "Given a word as a string, returns relationships from the Word Graph"
        }
      ],
      "path": "/word.{format}/{word}/relatedWords",
      "description": ""
    },
